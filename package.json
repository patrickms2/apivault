{
  "name": "integration-settings",
  "version": "1.0.0",
  "description": "An Apideck sample for managing integration settings with the Apideck Vault API.",
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./ --ext ts,js",
    "lint:fix": "eslint ./ --ext ts,js --fix",
    "lint:cache": "eslint ./ --ext ts,js --cache --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "typecheck": "tsc",
    "postbuild": "./scripts/upload-sourcemaps.sh",
    "test:e2e": "jest --detectOpenHandles --verbose --reporters=default --selectProjects=E2E --maxConcurrency=100",
    "test:unit": "jest --detectOpenHandles --colors --verbose --reporters=default --collectCoverage --no-cache --selectProjects=Unit",
    "test:watch": "jest --verbose --bail --watch"
  },
  "author": "apideck-samples",
  "license": "Apache-2.0",
  "dependencies": {
    "@apideck/components": "^0.6.5",
    "@bugsnag/js": "^7.11.0",
    "@bugsnag/plugin-react": "^7.11.0",
    "@headlessui/react": "^0.3.2",
    "axios": "^0.21.3",
    "camelcase-keys": "^6.2.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "formik": "^2.2.9",
    "fuse.js": "^6.4.6",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mdi-react": "^7.4.0",
    "next": "^11.1.1",
    "next-session": "^3.4.0",
    "node-fetch": "^2.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hotkeys": "^2.0.0",
    "react-icons": "^4.2.0",
    "react-markdown": "^6.0.3",
    "react-select": "^4.3.1",
    "react-table": "^7.7.0",
    "react-transition-group": "^4.4.2",
    "react-waypoint": "^10.1.0",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.2",
    "@types/react-select": "^4.0.13",
    "@types/react-table": "^7.7.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vercel/node": "^1.9.1",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "bugsnag-sourcemaps": "^1.3.0",
    "eslint": "^7.20.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "latest",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expect-playwright": "^0.3.2",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-playwright-preset": "^1.4.7",
    "jest-process-manager": "^0.2.9",
    "lint-staged": ">=10",
    "next-page-tester": "^0.20.0",
    "playwright": "^1.8.1",
    "playwright-core": "^1.8.1",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^26.5.1",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "pre-push": "yarn test:unit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --config .prettierrc 'src/**/*.ts' --write",
      "eslint ./ --ext ts,js --cache --fix"
    ]
  }
}
